#!/bin/bash
#
# update_cdn

FILEPATH=`pwd`/$0; BASEDIR=`dirname $FILEPATH`; . $BASEDIR/vars; source $BASEDIR/functions
outfile="/tmp/deploy-${deploy_outfile_prefix}-update-cdn"
`touch $outfile`

export APPLICATION_ENV=$environment

cd $BASEDIR
cd ../
vendor/bin/pav-console collector:search:export
vendor/bin/pav-console collector:storage:export



rm -rf $outfile
exit 0