{
  "require": {
    "spryker/spryker": "dev-bugfix/transfer-getproperties",

    "psr/log": "~1.0",
    "propel/propel": "~2.0@dev",
    "silex/silex": "~1.3",
    "pimple/pimple": "~1.1",
    "mandrill/mandrill": "~1.0",
    "predis/predis": "~1.0",

    "guzzle/http": "~3.7, <= 3.8",
    "league/csv": "~8.0",

    "monolog/monolog": "~1.7",

    "php": ">=5.6.0",
    "ruflin/elastica": "~1.3.4",

    "symfony/console": "~2.6.9",
    "symfony/debug": "~2.6.9",
    "symfony/filesystem": "~2.6.9",
    "symfony/finder": "~2.6.9",
    "symfony/form": "~2.6.9",
    "symfony/http-foundation": "~2.6.9",
    "symfony/http-kernel": "~2.6.9",
    "symfony/options-resolver": "~2.6.9",
    "symfony/process": "~2.6.9",
    "symfony/property-access": "~2.6.9",
    "symfony/routing": "~2.6.9",
    "symfony/security": "~2.6.9",
    "symfony/serializer": "~2.6.9",
    "symfony/translation": "~2.6.9",
    "symfony/twig-bridge": "~2.6.9",
    "symfony/validator": "~2.6.9",
    "symfony/var-dumper": "~2.6.9",
    "symfony/yaml": "~2.6.9",
    "symfony-cmf/routing": "~1.0",

    "twig/twig": "1.20",

    "zendframework/zend-http": "~2.3.0",
    "zendframework/zend-config": "~2.3.0",
    "zendframework/zend-code": "2.3.9",
    "zendframework/zend-filter": "~2.3.0",
    "zendframework/zend-servicemanager": "~2.3.0",
    "zendframework/zend-eventmanager": "2.3.9",

    "nesbot/carbon": "1.13.0"
  },
  "require-dev": {
    "silex/web-profiler": "~1.0.2",
    "pdepend/pdepend": "~2.2.2",
    "phploc/phploc": "~2.1.5",
    "phpmd/phpmd": "2.3.2",
    "sebastian/phpcpd": "~2.0.2",
    "codeception/codeception": "~2.1.0",
    "fabpot/php-cs-fixer": "^1.9",
    "squizlabs/php_codesniffer": "~2.4.0",
    "sensiolabs/security-checker": "~v3.0.2",
    "sllh/composer-versions-check": "~v1.0.2"
  },
  "autoload": {
    "psr-0": {
      "": "src",
      "Unit": "tests",
      "YvesUnit": "tests",
      "Functional": "tests"
    },
    "psr-4": {
      "Spryker\\Refactor\\": "refactoring"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "use-include-path": true
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker.git"
    }
  ]
}
