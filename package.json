{
  "private": true,
  "name": "yves_zed",
  "version": "0.3.0",
  "description": "Gulp asset distribution for Yves & Zed",
  "author": {
    "name": "Spryker",
    "email": "info@spryker.com",
    "url": "http://spryker.com/"
  },
  "scripts": {
    "prespy-setup": "chmod -R u+x ./gulp/bash",
    "spy-setup": "./gulp/bash/setup.sh",
    "spy-zed": "./gulp/bash/zed.sh",
    "spy-yves": "./gulp/bash/yves.sh",
    "spy-assets": "./gulp/bash/assets.sh",
    "spy-task-zed": "npm install && gulp --gulpfile config/Zed/gulpfile.js --cwd .",
    "spy-task-yves": "npm install && gulp --gulpfile config/Yves/gulpfile.js --cwd .",
    "spy-install": "npm run spy-task-zed && npm run spy-task-yves"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "del": "^1.2.1",
    "glob": "^5.0.14",
