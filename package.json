{
  "private": true,
  "name": "yves_zed",
  "version": "0.1.0",
  "description": "Gulp asset distribution for Yves & Zed",
  "author": {
    "name": "Spryker",
    "email": "info@spryker.com",
    "url": "http://spryker.com/"
  },
  "scripts": {
    "spy-run-shop-zed"  : "gulp --gulpfile config/Zed/gulpfile.js --cwd .",
    "spy-run-shop-yves" : "gulp --gulpfile config/Yves/gulpfile.js --cwd .",
    "spy-install-core"  : "cd ./vendor/spryker/ui && npm run spy-build",
    "spy-install-shop"  : "npm install && npm run spy-run-shop-zed dist && npm run spy-run-shop-yves dist",
    "spy-install"       : "npm run spy-install-core && npm run spy-install-shop"
  },
  "devDependencies": {
    "gulp"            : "~3.8",
    "gulp-concat"     : "~2.4",
    "gulp-sass"       : "~1.3",
    "gulp-jshint"     : "~1.9",
    "gulp-uglify"     : "~1.1",
    "gulp-sourcemaps" : "~1.3",
    "gulp-rename"     : "~1.2",
    "browser-sync": "^2.6.1",
    "browserify": "^9.0.8",
    "globby": "^2.0.0",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-concat": "~2.4",
    "gulp-hb": "~2.2",
    "gulp-jshint": "~1.9",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "~1.3",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglify": "^1.2.0",
    "jquery": "~2.1.3",
    "merge-stream": "^0.1.7",
    "q": "~1.1",
    "require-dir": "^0.3.0",
    "through2": "^0.6.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
    "dependencies": {
        "jquery-ui": "^1.10.5",
        "slick-carousel": "^1.5.2"
  }
}