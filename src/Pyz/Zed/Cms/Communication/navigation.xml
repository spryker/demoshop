<?xml version="1.0" encoding="UTF-8"?>
<config>
    <cms>
        <label>CMS</label>
        <title>CMS</title>
        <bundle>Cms</bundle>
        <icon>fa-files-o</icon>
        <pages>
            <page>
                <label>Pages</label>
                <title>Pages</title>
                <bundle>cms</bundle>
                <controller>page</controller>
                <action>index</action>
                <icon>fa-file-o</icon>
            </page>
            <block>
                <label>Blocks</label>
                <title>Blocks</title>
                <bundle>cms</bundle>
                <controller>block</controller>
                <action>index</action>
                <icon>fa-cubes</icon>
