                <label>Images</label>
                <title>Images</title>
                <bundle>cms</bundle>
                <controller>image</controller>
                <action>index</action>
                <icon>fa-file-picture-o</icon>
            </images>
            <translations>
                <label>Translations</label>
                <title>Translations</title>
                <bundle>cms</bundle>
                <controller>translation</controller>
