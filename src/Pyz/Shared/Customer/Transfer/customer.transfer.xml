<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="CustomerInfo">
        <property name="isReturning" type="bool" />
    </transfer>

    <transfer name="CustomerLoginResult">
        <property name="customerTransfer" type="Customer" />
        <property name="customerInfoTransfer" type="CustomerInfo" />
    </transfer>

    <transfer name="Customer">
        <property name="magentoPasswordHash" type="string" />
    </transfer>

</transfers>

