{% block partials %}
    {% block beforePartials %}{% endblock %}
    {% for row in templateElements %}
        {% for element in row %}
            {% set partialName = '@cms/partial/' ~ element.yvesPartialName %}
            {% set block = CmsPage.block(element.row, element.column) %}
            {% include partialName with {'block': block} %}
        {% endfor %}
    {% endfor %}
    {% block afterPartials %}{% endblock %}
{% endblock %}
