{% use '@Application/layout/common/icons.twig' %}

{% set data = {
    keys: ['ard', 'bild', 'spiegel', 'newyorktimes', 'bunte', 'zdf']
} %}



<div class="press">
    <div class="press__inner">

        {% for key in data.keys %}
            <div class="press__item press__item--{{ key }}">
                <div class="press__icon">
                    {{ block('icon_' ~ key) }}
                </div>
            </div>
        {% endfor %}

    </div>
</div>