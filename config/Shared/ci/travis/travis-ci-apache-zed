<VirtualHost *:80>

  DocumentRoot %TRAVIS_BUILD_DIR%/public/Zed
  ServerName zed.de.spryker.dev

  <FilesMatch \.php$>
    SetHandler application/x-httpd-php
  </FilesMatch>

  <Directory "%TRAVIS_BUILD_DIR%">
      Options Indexes FollowSymLinks MultiViews ExecCGI
      AllowOverride All
      Order allow,deny
      allow from all
  </Directory>

</VirtualHost>
